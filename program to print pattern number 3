#include<stdio.h>
int main()
{
    int i, j, n, columns;
    printf("Enter number of columns:");
    scanf("%d",&n);
    columns=1;
    for(i=1;i<=n;i++)
    {
        for(j=1; j<=5; j++)
        {
            if((j==2||j==3||j==4)&&(i==2||i==3||i==4))
        {
          printf("0");
        }
        else
        {
            printf("1");
        }
        }
       printf("\n");
}
return 0;
}
