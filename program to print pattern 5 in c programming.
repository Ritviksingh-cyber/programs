
#include<stdio.h>
int main()
{
    int i, j, n, columns;
    printf("Enter number of columns:");
    scanf("%d",&n);
    columns=1;
    for(i=1;i<=n;i++)
    {
        for(j=1; j<=5; j++)
        {
            if((j%2!=0)&&(i%2!=0))
        {
          printf("1");
        }
        else
        {
            printf("0");
        }
        }
       printf("\n");
}
return 0;
}

