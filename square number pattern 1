#include<stdio.h>
int main()
{
    int i, j, n, columns;
    printf("Enter number of columns:");
    scanf("%d",&n);
  columns=1;
  for(i=1;i<n;i++)
    {
        for(j=1; j<=5; j++)
        {
            if(j%2!=0)
            {
                printf("0");
        }  
            else
         {   
            printf("1");
         }
        if(i <=5)
        {
          columns++;
        }
            printf("\n");
     return 0;
}
